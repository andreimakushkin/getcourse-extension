{
  "name": "GetCourse player customize",
  "description": "Hotkeys & Customize video player on getcourse.ru",
  "version": "1.0.0",
  "browser_action": {
    "default_popup": "./html/popup.html"
  },
  "permissions": [
    "frames",
    "storage",
    "webRequest",
    "webRequestBlocking",
    "https://*.getcourse.ru"
  ],
  "icons": {
    "16": "./icons/icon16.png",
    "48": "./icons/icon48.png",
    "128": "./icons/icon128.png"
  },
  "content_scripts": [
    {
      "matches": ["*://*.getcourse.ru/*"],
      "js": ["./js/content.js"],
      "all_frames": true
    }
  ],
  "manifest_version": 2
}